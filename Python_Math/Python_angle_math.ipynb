   "source": [
    "import math\n",
    "\n",
    "AB = int(input())\n",
    "BC = int(input())\n",
    "\n",
    "hypotenuse  = math.sqrt(AB**2 + BC**2)\n",
    "hypotenuse  = hypotenuse /2.0\n",
    "adj = BC/2.0\n",
    "\n",
    "output = int(round(math.degrees(math.acos(adj/hypotenuse ))))\n",
    "\n",
    "output = str(output)\n",
    "\n",
    "print(output+u'\\N{DEGREE SIGN}')"
   ]
