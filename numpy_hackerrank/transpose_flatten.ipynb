   ],
   "source": [
    "import numpy\n",
    "n,m = map(int,input().split())\n",
    "ar = []\n",
    "for i in range(n):\n",
    "    row = list(map(int,input().split()))\n",
    "    ar.append(row)\n",
    "np_ar = numpy.array(ar)\n",
    "print(numpy.transpose(np_ar))\n",
    "print(np_ar.flatten())"
   ]
